    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 65ae8708f2864e9479e7e6aa0a97657b, type: 2}
    dialogue: 'Who will come out guilty in the vote?

      The answer is...!'
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 0}
    dialogue: (Video Jackpot)
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 65ae8708f2864e9479e7e6aa0a97657b, type: 2}
    dialogue: 'Yahoo! You''re totally right!

      The person who killed Sayaka
      Maizono in this murder case...

'
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 65ae8708f2864e9479e7e6aa0a97657b, type: 2}
    dialogue: ...was Leon Kuwata!
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 9e6a6af2bd2d0044e9384b38532bd06c, type: 2}
