    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 65ae8708f2864e9479e7e6aa0a97657b, type: 2}
    dialogue: 'If ya disrupt the peace, ya get punished!

      Isn''t that one of
      society''s rules?'
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 9e6a6af2bd2d0044e9384b38532bd06c, type: 2}
    dialogue: '(scream)

      N-No...! No!

      Lemme outta here!'
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 65ae8708f2864e9479e7e6aa0a97657b, type: 2}
    dialogue: 'For the Super High School-Level Baseball Player, Leon Kuwata,

      I''ve
      prepared a special little punishment!'
    imageIntroCharacter: {fileID: 0}
    isIntroCharacter: 0
  - speaker: {fileID: 11400000, guid: 9e6a6af2bd2d0044e9384b38532bd06c, type: 2}
    dialogue: '(scream)

